{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/force/local_force_scheme_main.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Current File with profiler",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "module": "cProfile",
      "args": [
        "-o",
        "/tmp/tmp.prof",
        "${workspaceFolder}/force/local_force_scheme_main.py"
      ]
    }
  ]
}
